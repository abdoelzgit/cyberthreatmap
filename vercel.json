{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "functions": {
    "app/api/**/*.js": {
      "runtime": "@vercel/node"
    }
  },
  "rewrites": [
    {
      "source": "/socket.io/:path*",
      "destination": "${SOCKET_SERVER_URL}/socket.io/:path*"
    }
  ]
}
